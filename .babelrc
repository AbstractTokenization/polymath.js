{
  "presets": [
    "flow",
    "stage-0",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-react-jsx",
    "syntax-async-functions",
    "transform-regenerator",
    "transform-runtime"
  ]
}